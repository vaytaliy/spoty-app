{
  "SpotifyDiscoveryDatabaseSettings": {
    "AccountCollectionName": "Account",
    "PlayedMusicCollectionName": "PlayedMusic",
    "RoomCollectionName": "Room",
    "DbURL": "mongodb://localhost:27018",
    "DbHost": "localhost",
    "DbPort": "27018",
    "InMemoryDatabaseConnection": "localhost:6000,abortConnect=false",
    "DatabaseName": "SpotifyDiscovery",
    "RedirectUri": "http://localhost:8006/discovery/authorization"
  },
  "Hosting": {
    "BaseURL": "http://localhost:8006"
  },
  "Spotify": {
    "AuthorizationLink": "https://accounts.spotify.com/authorize",
    "RedirectPath": "discovery/authorization"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [],
    "EndpointWhitelist": [ "*:/playerHub/*", "get:/static/*", "get:/manifest.json", "get:/favicon.ico" ],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "2s",
        "Limit": 10
      }
    ]
  },
  "Serilog": {
    "SeqUrl": "http://localhost:5341",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": {
      "Name": "MongoDBBson",
      "Args": {
        "databaseUrl": "mongodb://admin:password@localhost:27018",
        "collectionName": "DevLogs",
        "bulkSavePeriod": 5,
        "cappedMaxSizeMb": 1024
      }
    }
  }
}
